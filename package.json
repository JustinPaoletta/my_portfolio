{
  "name": "my-portfolio",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "analyze": "npm run build:analyze && npx vite-bundle-visualizer",
    "build": "npm run type-check && npx vite build",
    "build:analyze": "npm run type-check && ANALYZE=true npx vite build",
    "clean": "rm -rf node_modules dist coverage .vite .lighthouseci playwright-report test-results",
    "clean:cache": "rm -rf dist coverage .vite .lighthouseci playwright-report test-results",
    "images:optimize": "tsx scripts/optimize-images.ts",
    "lighthouse": "npx @lhci/cli autorun",
    "lint:ci": "npm run type-check && eslint . && prettier --check .",
    "lint:fix": "npm run type-check && eslint . --fix && prettier --write .",
    "prepare": "husky",
    "release": "commit-and-tag-version",
    "release:dry-run": "commit-and-tag-version --dry-run",
    "release:major": "commit-and-tag-version --release-as major",
    "release:minor": "commit-and-tag-version --release-as minor",
    "release:patch": "commit-and-tag-version --release-as patch",
    "sitemap:generate": "tsx plugins/vite-plugin-sitemap.ts",
    "start:dev": "npx vite",
    "start:prod": "npm run build && npx vite preview --strictPort --port 4173",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:coverage:ui": "vitest --coverage --ui",
    "test:coverage:watch": "vitest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:ui": "playwright test --ui",
    "test:ui": "vitest --ui",
    "test:unit": "vitest run",
    "type-check": "tsc -b --noEmit",
    "type-check:watch": "tsc -b --noEmit --watch"
  },
  "dependencies": {
    "@newrelic/browser-agent": "^1.303.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-helmet-async": "^2.0.5",
    "valibot": "^1.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@eslint/js": "^9.39.1",
    "@lhci/cli": "^0.15.1",
    "@playwright/test": "^1.56.1",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitest/coverage-v8": "^4.0.13",
    "@vitest/ui": "^4.0.5",
    "commit-and-tag-version": "^12.6.0",
    "dotenv": "^17.2.3",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "jsdom": "^27.2.0",
    "lint-staged": "^16.2.7",
    "prettier": "^3.6.2",
    "sharp": "^0.34.5",
    "tsx": "^4.20.6",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.47.0",
    "vite": "npm:rolldown-vite@7.1.14",
    "vite-plugin-pwa": "^1.1.0",
    "vitest": "^4.0.5",
    "workbox-window": "^7.4.0"
  },
  "overrides": {
    "chrome-launcher": "^1.2.1",
    "magic-string": "^0.30.0",
    "react-helmet-async": {
      "react": "^19.1.1"
    },
    "tmp": ">=0.2.4"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  }
}
